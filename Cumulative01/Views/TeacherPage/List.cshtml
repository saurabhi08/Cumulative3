@model IEnumerable<Cumulative01.Models.Teacher>
<div>
	<a style="display:inline-block; margin-bottom:20px; padding:8px 16px; background-color:#28a745; color:white; text-decoration:none; border-radius:4px;" href="/TeacherPage/New">
		Add A Teacher into the Database
	</a>



</div>


<form>
	<input type="text" name="SearchKey" />
	<input type="submit" name="SearchKey" value="Search" />



</form>
@foreach (Teacher teacher in Model)
{
	<div><a href="/TeacherPage/Show/@teacher.TeacherId"> @teacher.TeacherFirstName @teacher.TeacherLastName </a></div>
}